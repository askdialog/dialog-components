{
  "name": "@askdialog/dialog-components",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Dialog UI Components Monorepo",
  "scripts": {
    "build": "nx run-many -t build",
    "build:sdk": "nx build @askdialog/dialog-sdk",
    "build:vue": "nx build @askdialog/dialog-vue",
    "build:react": "nx build @askdialog/dialog-react",
    "build:affected": "nx affected -t build",
    "dev": "nx dev @askdialog/vue-example",
    "dev:vue-example": "nx dev @askdialog/vue-example",
    "dev:react-example": "nx dev @askdialog/react-example",
    "lint": "nx run-many -t lint",
    "lint:affected": "nx affected -t lint",
    "lint:fix": "nx run-many -t lint -- --fix",
    "format": "nx run-many -t lint -- --fix",
    "format:check": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "clean": "nx run-many -t clean",
    "graph": "nx graph",
    "publish:sdk": "pnpm --filter @askdialog/dialog-sdk publish",
    "publish:vue": "pnpm --filter @askdialog/dialog-vue publish",
    "publish:react": "pnpm --filter @askdialog/dialog-react publish",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "pnpm build && changeset publish",
    "changeset:status": "changeset status"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.39.1",
    "@nx/eslint": "^20.8.2",
    "@nx/eslint-plugin": "^20.8.2",
    "@nx/js": "22.1.2",
    "@nx/react": "^22.1.2",
    "@nx/vite": "^20.8.2",
    "@nx/web": "20.8.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^3.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "^20.8.2",
    "prettier": "^3.6.2",
    "tslib": "^2.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.0"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "nx": {
    "includedScripts": []
  }
}
