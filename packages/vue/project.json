{
  "name": "@askdialog/dialog-vue",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/vue/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": ["^build"],
      "options": {
        "command": "pnpm run build",
        "cwd": "packages/vue"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run dev",
        "cwd": "packages/vue"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run lint",
        "cwd": "packages/vue"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run clean",
        "cwd": "packages/vue"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run preview",
        "cwd": "packages/vue"
      }
    }
  },
  "tags": ["type:library", "scope:vue"],
  "implicitDependencies": ["@askdialog/dialog-sdk"]
}
